    "def rotate_right(d,arr):\n",
    "    n = len(arr)\n",
    "    d = d%n\n",
    "    arr = arr[-d:]+arr[:-d]\n",
    "    return arr\n",
    "rotate_right(2,[1,2,3,4,5])"
   ]
