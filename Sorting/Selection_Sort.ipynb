   "source": [
    "l1 = [10,56,5,54,3,33,222,21,12,2,2,33]\n",
    "size = len(l1)\n",
    "for i in range(size-1):\n",
    "    min = l1[i]\n",
    "    pos = i\n",
    "    for j in range(i+1,size):\n",
    "        if l1[j]<min:\n",
    "            min = l1[j]\n",
    "            pos = j\n",
    "    if i!=pos:\n",
    "        l1[i],l1[pos]=l1[pos],l1[i]\n",
    "l1"
